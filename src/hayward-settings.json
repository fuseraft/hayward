{
    "name": "hayward",
    "version": "1.2.1",
    "file_extensions": {
      "primary": ".kiwi",
      "minified": ".min.kiwi",
      "recognized": [
        ".min.kiwi",
        ".kiwi",
        ".min.ü•ù",
        ".ü•ù"
      ]
    },
    "standard_library": [
      {
        "autoload": true,
        "path": "../lib",
        "include_subdirectories": true
      }
    ],
    "safemode": true,
    "debug": {
      "cli_args": [
        "~/github/hayward/hello",
        "20",
        "--stdlib-path",
        "~/github/hayward/lib"
      ]
    },
    "environment_variables": {
      "_KIWI": ""
    },
    "crashdump_path": "../hayward_crash.log"
  }
  