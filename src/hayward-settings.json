{
    "name": "hayward",
    "version": "1.1.0",
    "file_extensions": {
      "primary": ".hayward",
      "minified": ".min.hayward",
      "recognized": [
        ".min.hayward",
        ".hayward",
        ".min.hay",
        ".hay",
        ".kiwi",
        ".min.🥝",
        ".🥝"
      ]
    },
    "standard_library": [
      {
        "autoload": true,
        "path": "../lib",
        "include_subdirectories": true
      }
    ],
    "safemode": true,
    "debug": {
      "cli_args": [
        "~/GitHub/hayward/scripts/cellular_automata/life",
        "20",
        "--stdlib-path",
        "~/GitHub/hayward/lib"
      ]
    },
    "environment_variables": {
      "_KIWI": ""
    },
    "crashdump_path": "../hayward_crash.log"
  }
  